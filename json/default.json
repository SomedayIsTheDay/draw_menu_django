{
  "menu": {
    "display": "flex",
    "align-items": "center",
    "justify-content": "space-between",
    "background-color": "#333",
    "color": "#fff",
    "font-size": "18px",
    "font-weight": "bold"
  },
  "menu-item": {
    "display": "inline-block",
    "padding": "10px 20px",
    "color": "#fff",
    "text-decoration": "none",
    "&:hover": {
      "background-color": "#444"
    }
  },
  "logo": {
    "height": "50px",
    "& img": {
      "height": "100%",
      "width": "auto"
    }
  }
}